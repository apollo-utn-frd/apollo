model {
  Seguimiento seguimiento
}

json {
  id seguimiento.id

  seguido g.render(
    template: '/usuario/usuario_min',
    model: [usuario: seguimiento.seguido]
  )

  seguidor .render(
    template: '/usuario/usuario_min',
    model: [usuario: seguimiento.seguidor]
  )

  dateCreated seguimiento.dateCreated
}
