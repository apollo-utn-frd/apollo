model {
  Comentario comentario
}

json {
  id comentario.id

  viaje g.render(
    template: '/id',
    model: [object: comentario.viaje]
  )

  usuario g.render(
    template: '/id',
    model: [object: comentario.usuario]
  )

  contenido comentario.contenido
  dateCreated comentario.dateCreated
}
