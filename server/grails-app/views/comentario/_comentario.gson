model {
  Comentario comentario
}

json {
  id comentario.id

  viaje g.render(
    template: '/viaje/viaje_min',
    model: [viaje: comentario.viaje]
  )

  usuario g.render(
    template: '/usuario/usuario_min',
    model: [usuario: comentario.usuario]
  )

  contenido comentario.contenido
  dateCreated comentario.dateCreated
}
